{
    "name": "SoundClone",
    "version": "1.0.0",
    "description": "SoundClone: The SoundCloud clone",
    "main": "server/server.js",
    "type": "module",
    "scripts": {
        "start": "node server",
        "server": "nodemon server --ignore client",
        "client": "npm start --prefix client",
        "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client",
        "dev": "concurrently \"cd client && npm run start\" \"cd server && nodemon server.js\" "
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "concurrently": "^7.1.0",
        "nodemon": "^2.0.16"
    },
    "dependencies": {
        "aws-sdk": "^2.1131.0",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "express": "^4.18.1",
        "express-async-errors": "^3.1.1",
        "http-status-codes": "^2.2.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.3.2",
        "morgan": "^1.10.0",
        "multer": "^1.4.4",
        "react-icons": "^4.3.1",
        "validator": "^13.7.0"
    }
}
