{
    "name": "SoundClone",
    "version": "1.0.0",
    "description": "SoundClone: The SoundCloud clone",
    "main": "server/server.js",
    "type": "module",
    "scripts": {
        "start": "node server",
        "server": "nodemon server --ignore client",
        "client": "npm start --prefix client",
        "install": "cd server && npm i && cd ../client && npm i",
        "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client",
        "dev": "concurrently \"cd client && npm run start\" \"cd server && nodemon server.js\" ",
        "seed": "cd server && node seeders/seed.js"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "concurrently": "^7.1.0",
        "nodemon": "^2.0.16"
    },
    "dependencies": {
        "apollo-server": "^3.5.0",
        "apollo-server-errors": "^3.3.1",
        "apollo-server-express": "^2.12.0",
        "aws-sdk": "^2.1131.0",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "graphql": "^15.7.2",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.3.2",
        "morgan": "^1.10.0",
        "multer": "^1.4.4",
        "react-icons": "^4.3.1",
        "styled-components": "^5.3.5"
    }
}
